<h1><%= t(:title) %></h1>

<% form_for(@user_session) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :email, t(:email) %>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password, t(:password) %>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit t(:submit) %>
  </p>
<% end %>

